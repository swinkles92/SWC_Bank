@page
@model Bank.Web.Pages.LocationsModel
<div class="row">
    <h1 class="display-2">Locations</h1>
    <table class="table">
        <thead class="thead-inverse">
            <tr><th>Branch Location</th></tr>
        </thead>
        <tbody>
        @if(Model.Locations is not null)
        {
                @foreach(string name in Model.Locations)
                {
                    <tr><td>@name</td></tr>
                }
        }
        </tbody>
    </table>
</div>

